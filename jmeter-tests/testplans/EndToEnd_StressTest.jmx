            <stringProp name="parameters"></stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">String statusValue = vars.get(&quot;statusValue&quot;);
if (statusValue.equals(&quot;1&quot;)) {
    vars.put(&quot;status_1&quot;, &quot;PROCESSING&quot;);
} else if (statusValue.equals(&quot;2&quot;)) {
    vars.put(&quot;status_2&quot;, &quot;SHIPPED&quot;);
} else if (statusValue.equals(&quot;3&quot;)) {
    vars.put(&quot;status_3&quot;, &quot;DELIVERED&quot;);
} else {
    vars.put(&quot;status_4&quot;, &quot;CANCELLED&quot;);
}</stringProp>
