            <stringProp name="Argument.value">${__P(outbox.service.host,localhost)}</stringProp>
            <stringProp name="Argument.metadata">=</stringProp>
          </elementProp>
          <elementProp name="OUTBOX_SERVICE_PORT" elementType="Argument">
            <stringProp name="Argument.name">OUTBOX_SERVICE_PORT</stringProp>
            <stringProp name="Argument.value">${__P(outbox.service.port,8081)}</stringProp>
            <stringProp name="Argument.metadata">=</stringProp>
